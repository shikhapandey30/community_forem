<%if @like.present? %>
$("#like_image_post_<%= params[:id] %>").css('background-color', '#0bb4d3');
$("#like_image_post_liked_<%= params[:id] %>").css('background-color', '#0bb4d3');
<%else%>
$("#like_image_post_<%= params[:id] %>").css('background-color', '#fff');
$("#like_image_post_liked_<%=  params[:id] %>").css('background-color', '#fff');
<% end %>
$("#post_like_count_<%= params[:id] %>").html('<%= @model.likes.count%>');
