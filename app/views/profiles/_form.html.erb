                  <option>University 1</option>
                </select>
              </div>
            </div>
            <div class="col-sm-6 pr10">
              <label>Year</label>
              <div class="selctFormBox">
                <input value="2006" readonly="" class="selectInput" type="text">
                <select class="select">
                  <option>2006</option>
                </select>
              </div>
            </div>
            <div class="col-sm-6 pl10">
