                  <input type="text" value="Select" readonly class="selectInput">
                  <select class="select">
                    <option>Select</option>
                  </select>
                </div> -->
              </div>
              <div class="col-sm-6 pr10">
                <label>Street</label>
                <%= p.text_field :street, class: "profile inputTextW small" %>
                <!-- <div class="selctFormBox">
                  <input type="text" value="Select" readonly class="selectInput">
                  <select class="select">
                    <option>Select</option>
                  </select>
                </div> -->
              </div>
              <div class="col-sm-6 pl10">
                <label>Zipcode</label>
                <%= p.text_field :post_code, class: "profile inputTextW small", placeholder: "10001" %>
              </div>
              <div class="col-sm-6 pr10">
                <label>Gender</label>
                <div class="selctFormBox">                  
                  <%= p.text_field :gender, value: "Select", class: "profile selectInput" %>
                  <select class="select">
                    <option>Male</option>
                    <option>Female</option>
                  </select>
                </div>
              </div>
              <div class="col-sm-6 pl10">
                <label>Phone</label>
                <%= p.text_field :phone_no, class: "profile inputTextW small", placeholder: "0123456789" %>
              </div>
              <div class="col-sm-6 pr10">
                <label>Select Type</label>
                <div class="selctFormBox">
                  <input type="text" value="Individual" readonly class="selectInput">
                  <select class="select">
                    <option>Individual</option>
                  </select>
                </div>
              </div>
              <div class="col-sm-6 pl10">
                <label>Date of Birth</label>
                <input type="text" placeholder="987654321" class="inputTextW small date fullWdth">
              </div>
              <div class="col-sm-6 pr10">
                <label>Screen Name</label>
                <input type="text" placeholder="john2015" class="inputTextW small  "> 
              </div>
              <div class="col-sm-6 pl10">
                <label>Location</label>
                <input type="text" placeholder="South America" class="inputTextW small  ">
              </div>
              <div class="col-sm-12">
                <input type="submit" value="Save " class="blueBtn">
                <a href="#" class="grayBtn">Cancel</a> 
              </div>
            </div>
          <%end%>  
