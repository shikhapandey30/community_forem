      <div class="topRightNav">
        <span class="blueColor rightMenu"> Menu  <img src="../assets/images/down.png" alt="" class=""></span>
        <ul>
          <li class="activeMenu"><a href="#">News Feed</a></li>
          <li><a href="#">Hot Topics</a></li>
          <li><a href="#">Followers (25)</a></li>
          <li><a href="#">Following (100)</a></li>
          <li><a href="#">More <img src="../assets/images/arrowBlack.png" alt=""> </a></li>
        </ul>
      </div>
    </div>

    <%= nested_form_for(current_user, url: "#-", remote: :true) do |f| %>
      <div class="clearfix innerWIdth550 tac ">
        <div class="clearfix whiteBox mt30">
          <%= f.fields_for :profile do |p| %>
            <div class="clearfix formOuterPost tal">
              <%= p.file_field :image, class: "profile", style: "visibility: hidden; width: 1px; height: 1px" %>
              <!-- <input id="Photo" name="Photo" style="visibility: hidden; width: 1px; height: 1px" multiple="" type="file"> -->
              <div class="uploadPhoto" onclick="document.getElementById('user_profile_attributes_image').click(); return false">
                <div class="reletive">
                  <img src="../assets/images/camera.png" alt="">
                  <div>Change Photo</div>
                </div>
              </div>
              <div class="col-sm-6 pr10">
                <label>First Name</label>
                <%= p.text_field :first_name, class: "profile inputTextW small", placeholder: "John" %>
              </div>
              <div class="col-sm-6 pl10">
                <label>Last Name</label>
                <%= p.text_field :last_name, class: "profile inputTextW small", placeholder: "John" %>
              </div>
              <div class="col-sm-12">
                <div class="blackHD">Address</div>
              </div>
              <div class="col-sm-6 pr10">
                <label class="">Country</label>
