              <div class="grayHD">Add category </div>
            </div>
            <div class="col-sm-12 addSkills">
              <label class="font18">Select Category </label><br>
            </div>
            <div class="col-sm-6 pr10 categoriesChk">
              <label> <input type="checkbox">   Entertainment</label>
              <label> <input type="checkbox">   Politics</label>
              <label> <input type="checkbox">  Technology</label>
              <label> <input type="checkbox"> Sports</label>
              <label> <input type="checkbox"> Music</label>
              <label> <input type="checkbox"> Religion &amp; Ideas</label>
              <label> <input type="checkbox"> Politics &amp; Government</label>
              <label> <input type="checkbox"> Travel &amp; Lifestyle</label>
