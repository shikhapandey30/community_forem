<span class="userNAme"><%= model_type(object.first.class.name) %></span>
<div class="clearfix"></div>
<% object.each do |follow|%>
  <% if (follow.class.name == "Community" || follow.class.name == "Group" || follow.class.name == "MeetingRoom") %>
    <div class="whiteBox borderTop col-sm-6 pb0 fl">   
      <a href="<%= model_path(follow) %>" class="block mt20 grpImg text-center"><%=image_tag follow.img , :title =>follow.try(:topic)%></a>
      <a href="<%= model_path(follow) %>" class="block groupName width"> <%=follow.try(:topic)%></a>
      <div class="virwJoinBtn">
        <a class="btmBtn" href="#" > <%= follow.members.try(:count)%> <%= follow.members.try(:count) >1 ? 'Members' : 'Member' %> </a>
        <a class="btmBtn" href="#"> Join </a>           
      </div>
    </div>
  <%elsif follow.class.name == "Post"%>
    <div class="whiteBox borderTop col-sm-6 pb0 fl">
      <a href="<%= model_path(follow) %>" class="block mt20 grpImg text-center"><%=image_tag follow.img , :title =>follow.try(:topic)%></a>
      <a href="<%= model_path(follow) %>" class="block groupName width"> <%=follow.try(:topic)%></a>
    </div>
  <% end %> 
<%end%>
